html
    head
        title Guess word by Person!
        script(defer src="client.js")

    body(id="body")

        - if (!responseObject)
            include partials/header
            h2 Get Tweets by Person
            input(type="text" id="textInput")
            input(type="button" onclick="makeRequest()" value="Make Request")
            div(id="responseDiv")
        - else
            - let t = JSON.stringify(responseObject)
            div= t